#include"LEDS.h"

void Led_Init(void)
{

	/*
	 * Configure the led as output pin
	 */
	LED_DIRECTION_PORT = SET_BIT(LED_DIRECTION_PORT, LED_OUTPUT_PIN);


}/*end of Led_Init*/
