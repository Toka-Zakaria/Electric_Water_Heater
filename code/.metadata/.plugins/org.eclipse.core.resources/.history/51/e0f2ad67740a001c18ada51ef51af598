#include"LEDS.h"

void Leds_Init(const LEDs_TypeConfig * config_ptr)
{


	switch((config_ptr->Leds_number))
	{

	case one:

		LED_DIRECTION_PORT = SET_BIT(LED_DIRECTION_PORT, LED_OUTPUT_PIN);

		break;



	}/*end of switch case*/



}/*end of Leds_Init function*/
